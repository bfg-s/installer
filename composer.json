{
    "name": "bfg/installer",
    "type": "bfg-app",
    "description": "Simplify the installation process for certain packages",
    "keywords": [
        "laravel",
        "doc",
        "block"
    ],
    "homepage": "https://github.com/bfg/installer",
    "license": "MIT",
    "authors": [
        {
            "name": "bfg",
            "email": "bfg.script@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0",
        "bfg/entity": "*"
    },
    "require-dev": {

    },
    "autoload": {
        "psr-4": {
            "Bfg\\Installer\\": "src"
        },
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {

        }
    },
    "scripts": {

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bfg\\Installer\\ServiceProvider"
            ],
            "aliases": {
                "Installer": "Bfg\\Installer\\Facade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
